{
  "name": "gutsy-reviews",
  "version": "1.0.0",
  "description": "The reviews microservice for the Gutsy app",
  "main": "index.js",
  "scripts": {
    "migrate": "npx knex migrate: latest",
    "seed": "npx knex seed:run",
    "react-dev": "webpack -d --watch --display-error-details",
    "server-dev": "nodemon server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "restart": "pm2 startOrRestart ecosystem.config.js",
    "update": "pm2 kill && pm2 start ecosystem.config.js --env production",
    "deploy": "pm2 deploy ecosystem.config.js production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microdynamicservices/gutsy-reviews.git"
